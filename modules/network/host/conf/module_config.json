{
  "config": {
    "meta": {
      "name": "host",
      "display_name": "Host",
      "description": "Used to access host level networking operations"
    },
    "network": {
      "interface": "veth0",
      "enable_wan": false,
      "ip_index": 8,
      "host": true
    },
    "grpc":{
      "port": 5001
    },
    "docker": {
      "depends_on": "base",
      "mounts": [
        {
          "source": "runtime/network",
          "target": "/runtime/network"
        }
      ]
    }
  }
}